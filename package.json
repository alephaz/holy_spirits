{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "build-video-field": "cd nova-components/VideoField && npm run dev",
        "build-video-field-prod": "cd nova-components/VideoField && npm run prod",
        "build-donations": "cd nova-components/Donations && npm run dev",
        "build-donations-prod": "cd nova-components/Donations && npm run prod",
        "build-MultiDateSelector": "cd nova-components/MultiDateSelector && npm run dev",
        "build-MultiDateSelector-prod": "cd nova-components/MultiDateSelector && npm run prod",
        "build-CustomVideo": "cd nova-components/CustomVideo && npm run dev",
        "build-CustomVideo-prod": "cd nova-components/CustomVideo && npm run prod",
        "build-create-donated-user": "cd nova-components/CreateDonatedUser && npm run dev",
        "build-create-donated-user-prod": "cd nova-components/CreateDonatedUser && npm run prod",
        "build-Nova-translatable": "cd nova-components/NovaTranslatable && npm run dev",
        "build-Nova-translatable-prod": "cd nova-components/NovaTranslatable && npm run prod",
        "build-send-test-mail": "cd nova-components/SendTestMail && npm run dev",
        "build-send-test-mail-prod": "cd nova-components/SendTestMail && npm run prod"
    },
    "devDependencies": {
        "axios": "^0.18",
        "bootstrap": "^4.0.0",
        "browser-sync": "^2.26.3",
        "browser-sync-webpack-plugin": "^2.2.2",
        "cross-env": "^5.1",
        "jquery": "^3.2",
        "laravel-mix": "^2.0",
        "lodash": "^4.17.5",
        "popper.js": "^1.12",
        "vue": "^2.5.7"
    },
    "dependencies": {
        "js-cookie": "^2.2.0",
        "vue-video-player": "^5.0.2",
        "vue-youtube": "^1.3.0"
    }
}